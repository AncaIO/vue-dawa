{
  "name": "@ancaio/vue-dawa",
  "version": "2.1.1",
  "description": "A Vue2 component for one-line address autocomplete using Danmarks Adresser WEB API.",
  "main": "./dist/vue-dawa.umd.js",
  "unpkg": "./dist/vue-dawa.umd.min.js",
  "browser": {
    "./sfc": "./src/components/VueDawa.vue"
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "config": {
    "prettyName": "Vue DAWA"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AncaIO/vue-dawa.git"
  },
  "keywords": [
    "vuejs",
    "autocomplete",
    "address",
    "denmark",
    "dk"
  ],
  "author": "Anca I/O",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AncaIO/vue-dawa/issues"
  },
  "homepage": "https://github.com/AncaIO/vue-dawa#readme",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-lib": "vue-cli-service build --target lib --name vue-dawa 'src/components/VueDawa.vue'",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "test": "npm run test:unit"
  },
  "dependencies": {
    "vue": "^2.6.12"
  },
  "peerDependencies": {
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.8",
    "@vue/cli-plugin-e2e-cypress": "^4.5.8",
    "@vue/cli-plugin-eslint": "^4.5.8",
    "@vue/cli-plugin-unit-mocha": "^4.5.8",
    "@vue/cli-service": "^4.5.8",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "^1.1.1",
    "axios": "^0.21.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "braces": "^2.3.2",
    "chai": "^4.1.2",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.2",
    "eslint-plugin-vue": "^6.2.2",
    "expect": "^24.9.0",
    "jsdom": "^15.2.1",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^8.2.1",
    "minimist": "^1.2.5",
    "mocha": "^6.2.3",
    "mocha-webpack": "^1.1.0",
    "mochapack": "^2.0.6",
    "sass": "^1.28.0",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.12",
    "webpack-node-externals": "^1.7.2"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
